CREATE SEQUENCE  "SCOTT"."PID_SEQ"  MINVALUE 1 MAXVALUE 100 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE ;


  CREATE TABLE "SCOTT"."PERSON_INFO" 
   (	"PID" NUMBER(12,0) NOT NULL ENABLE, 
	"PNAME" VARCHAR2(30 BYTE), 
	"FNAME" VARCHAR2(20 BYTE), 
	"MS" VARCHAR2(20 BYTE), 
	"SPOUSE" VARCHAR2(20 BYTE), 
	"KIDSCOUNT" NUMBER(20,0), 
	"WHENTOMARRY" VARCHAR2(20 BYTE), 
	"WHYTOMARRY" VARCHAR2(20 BYTE), 
	 CONSTRAINT "TABLE1_PK" PRIMARY KEY ("PID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "USERS"  ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "USERS" ;
 
